public Id insertCourse(String name, String details, Integer credits) {
            // code here
            if (details != 1 && details != 2 && details != 3 &&  details != 4)
                return null
            apxio__Course__c nuevoCurso = new apxio__Course__c();
            apxio__Course__c.name = name;
            apxio__Course__c.apxio__Course_Details__c = details;
            apxio__Course__c.apxio__Credits__c = credits;
            insert nuevoCurso;
            return apxio__Course__c.Id;
        }