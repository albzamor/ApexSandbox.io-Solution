public List<apxio__Student__c> selectStudentsWithoutContactInfo() {
    // code here
    List<apxio__Student__c> missingStudent = [Select Id, Name from apxio__Student__c where apxio__Active__c = true AND (apxio__Email__c OR apxio__Student__c.apxio__Phone__c) order by Name ];
    return missingStudent;
}