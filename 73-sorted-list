public boolean isSorted(List<Integer> numbers) {
    //code here
    integer a = 0;
    for (Integer i = 0; i<numbers.size()-1;i++){
        
        while (numbers[i+1] > numbers[i])
            a++;
        while (numbers[i+1] < numbers[i])
            a++;
    }
    if (a == numbers.size())
        return true;
    false;
}