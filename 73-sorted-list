public boolean isSorted(List<Integer> numbers) {
    //code here
    integer a = 0;
    if (numbers.size()==1)
        return true;
    integer size = numbers.size();
    for (Integer i = 0; i<size-1;i++){
        
        while (numbers[i+1] > numbers[i])
            a++;
        while (numbers[i+1] < numbers[i])
            a++;
        while (numbers[i+1] == numbers[i])
            a++;
    }
    if (a == size)
        return true;
    return false;
}