public void enrollStudents(List<String> emails, String className) {
    // code here
    list <apxio__Class_Enrollment__c> enrolos = new list <apxio__Class_Enrollment__c>();
    for (String emal :emails )
    {
        List<apxio__Student__c> estu = [select id from apxio__Student__c where apxio__Email__c = :emal];
        id idStudent = estu[0].Id;
        apxio__Class_Enrollment__c enrol = new apxio__Class_Enrollment__c();
        enrol.apxio__Student__c =idStudent;
        enrol.apxio__Offered_Class__c = className;
        enrolos.add(enrol);
    }
    insert enrolos;
}