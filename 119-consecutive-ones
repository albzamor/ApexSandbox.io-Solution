public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    Integer a = 0;
    Integer Max = 0;
    if (numbers.size())>0{
        for (Integer num : numbers)
        {
            while (num == 1)
                a++;

            if (a > Max)
                Max = a;
            a = 0;
        }   
    }
    return Max;
}